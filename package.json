{
  "name": "chat-widget",
  "version": "1.0.0",
  "description": "Chat Widget made with React.js",
  "main": "release/chat-widget.js",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack --mode=production",
    "tests": "jest --coverage",
    "test": "jest --watchAll --coverage"
  },
  "author": "Mauricio Martinez (github.com/mavriciomart)",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^24.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "react-test-renderer": "^16.7.0",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "css-loader": "^2.1.0",
    "dateformat": "^3.0.3",
    "eslint": "^5.12.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "store": "^2.0.12",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
